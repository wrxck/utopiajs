{
  "name": "@utopia/runtime",
  "version": "0.0.1",
  "description": "DOM renderer and component lifecycle for UtopiaJS",
  "type": "module",
  "license": "MIT",
  "author": "Matt <matt@matthesketh.pro>",
  "repository": {
    "type": "git",
    "url": "https://github.com/wrxck/utopiajs.git",
    "directory": "packages/runtime"
  },
  "homepage": "https://github.com/wrxck/utopiajs/tree/main/packages/runtime",
  "keywords": ["runtime", "dom", "renderer", "lifecycle", "directives", "utopiajs"],
  "engines": { "node": ">=20.0.0" },
  "publishConfig": { "access": "public" },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch"
  },
  "dependencies": {
    "@utopia/core": "workspace:*"
  }
}
