{
  "name": "@matthesketh/utopia-compiler",
  "version": "0.0.1",
  "description": "Compiler for .utopia single-file components",
  "type": "module",
  "license": "MIT",
  "author": "Matt <matt@matthesketh.pro>",
  "repository": {
    "type": "git",
    "url": "https://github.com/wrxck/utopiajs.git",
    "directory": "packages/compiler"
  },
  "homepage": "https://github.com/wrxck/utopiajs/tree/main/packages/compiler",
  "keywords": ["compiler", "sfc", "single-file-components", "parser", "utopiajs"],
  "engines": { "node": ">=20.0.0" },
  "publishConfig": { "access": "public" },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch"
  },
  "dependencies": {
    "@matthesketh/utopia-core": "workspace:*"
  }
}
